{# GTAG #}

{% if tails.gtag %}
	<!-- Google tag (gtag.js) -->
	<script async src="{{'https://www.googletagmanager.com/gtag/js?id='~ tails.gtag}}"></script>
	<script>
		window.dataLayer = window.dataLayer || [];
function gtag() {
dataLayer.push(arguments);
}
gtag('js', new Date());

gtag('config', {{ tails.gtag|json_encode() }});
	</script>
{% endif %}
